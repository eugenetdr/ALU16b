module compare16 (
  // compare module takes in input z,v,n, alufn[2:1]
  input z, 
  input v,
  input n,
  input alufn[6],
  output out[16] 
){
  always{
  out[15:0] = 16b0;

  case(alufn[2:1]){
  
  2b01: 
    out[0] = z;
    out[15:1] = 15b0;

  2b10:
    out[0] = n ^ v;
    out[15:1] = 15b0;

  2b11:
    out[0] = z | (n^v);
    out[15:1] = 15b0;    
  }





  }}